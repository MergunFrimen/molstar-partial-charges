{
    "name": "molstar-partial-charges",
    "version": "0.0.0",
    "description": "Molstar plugin for partial charges",
    "author": "Dominik Tich√Ω",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/MergunFrimen/molstar-partial-charges.git"
    },
    "bugs": {
        "url": "https://github.com/MergunFrimen/molstar-partial-charges/issues"
    },
    "keywords": [
        "molstar"
    ],
    "type": "module",
    "main": "./dist/molstar-partial-charges.umd.cjs",
